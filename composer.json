{
    "name": "deegital/laravel-trustup-io-microservice-model",
    "description": "A package to define microservice model.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Axel Marquet",
            "email": "axel.marquet@trustup.be"
        }
    ],
    "require": {
        "henrotaym/laravel-package-versioning-config": "^1.3"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Deegital\\LaravelTrustupIoMicroserviceModel\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Deegital\\LaravelTrustupIoMicroserviceModel\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Deegital\\LaravelTrustupIoMicroserviceModel\\Providers\\LaravelTrustupIoMicroserviceModelServiceProvider"
            ]
        }
    }
}
